<table class="table table-striped table-hover">
  <thead>
  <tr>
    <th><strong>Name</strong></th>
    <th><strong>Phone</strong></th>
    <th><strong>Date</strong></th>
  </tr>
  </thead>
  <tbody>
  <% @results.each do |row| %>
  <tr>
    <td><%= row['Name'] %></td>
    <td><%= row['Phone'] %></td>
    <td><%= row['Date'] %></td>
  </tr>
   <% end %>
  </tbody>
</table>